{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/health/index.js": {
      "maxDuration": 10,
      "runtime": "nodejs22.x"
    },
    "api/resume-analyze.js": {
      "maxDuration": 60,
      "runtime": "nodejs22.x"
    },
    "api/interview-analyze.js": {
      "maxDuration": 60,
      "runtime": "nodejs22.x"
    }
  }
}